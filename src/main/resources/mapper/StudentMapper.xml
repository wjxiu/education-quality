<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.wjxiu.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="com.github.wjxiu.DO.StudentDO">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="real_name" column="real_name" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="major_name" column="major_name" jdbcType="INTEGER"/>
            <result property="department_name" column="department_name" jdbcType="VARCHAR"/>
            <result property="enroll_year" column="enroll_year" jdbcType="INTEGER"/>
            <result property="email" column="email" jdbcType="VARCHAR"/>
            <result property="update_time" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="create_time" column="create_time" jdbcType="INTEGER"/>
            <result property="del_flag" column="del_flag" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,real_name,password,
        major_name,department_name,enroll_year,
        email,update_time,create_time,
        del_flag
    </sql>
</mapper>
